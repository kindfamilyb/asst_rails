<%= link_to "Home", root_path %>
<%= link_to "Upbit", upbit_accounts_path %>

<%# 매매내역 테이블 형태로 출력 %>
<table>
  <thead>
    <tr>
      <th>코인</th>
      <th>매도여부</th>
      <th>수익률</th>
      <th>유저ID</th>
      <th>내전략ID</th>
      <th>매도수량</th>
      <th>남은수량</th>
      <th>매매일시</th>
    </tr>
  </thead>
  <tbody>
    <% @trades.each do |trade| %>
      <tr>
        <td><%= trade.coin_symbol %></td>
        <td><%= trade.sold %></td>
        <td><%= trade.profit %></td>
        <td><%= trade.user_id %></td>
        <td><%= trade.my_strategy_info_id %></td>
        <td><%= trade.volume %></td>
        <td><%= trade.remaining_volume %></td>
        <td><%= trade.formatted_created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>





